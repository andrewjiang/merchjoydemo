      <div class="">
        <%= form_for(@order) do |f| %>

          <div class="field">
            <%= f.label :quantity %><br>
            <%= f.number_field :quantity %>
          </div>
          <div class="field">
            <%= f.label :canvas %><br>
            <%= f.text_field :canvas, :value=> @canvas %>
          </div>
          <div class="field">
            <%= f.label :image %><br>
            <%= f.text_field :image %>
          </div>
          <div class="field">
            <%= f.label :offset %><br>
            <%= f.text_field :offset %>
          </div>
          <div class="field">
            <%= f.label :startW %><br>
            <%= f.text_field :startW %>
          </div>
          <div class="field">
            <%= f.label :price %><br>
            <%= f.text_field :price %>
          </div>
          <div class="actions">
            <%= f.submit "new", :id=>"submit-new" %>
            <%= f.submit "save", :id=>"submit-save" %>
          </div>
          <br><br><br><br>
        <% end %>
